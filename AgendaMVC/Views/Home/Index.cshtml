@model List<AgendaMVC.Models.View.Registers>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <div class="row" style="display:flex">
        <div class="col-md-6">
            <h2>ASP.NET</h2>
            <p class="lead">Agenda</p>
        </div>
    </div>
</div>
<h2>Contactos</h2>

<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <tr>
                <th>Codigo</th>
                <th>Nombre</th>
                <th>Apellido</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nombre</td>
                    <td>@item.Apellido</td>
                </tr>
            }
        </table>
    </div>
</div>

